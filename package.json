    "@types/swagger-ui-react": "^5.18.0",
    "@upstash/redis": "^1.35.3",
    "argon2": "^0.40.3",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
