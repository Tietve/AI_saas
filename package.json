    "html2canvas": "^1.4.1",
    "ioredis": "^5.8.0",
    "jest": "^30.2.0",
    "jose": "^5.9.3",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.544.0",
    "mammoth": "^1.11.0",
    "next": "14.2.18",
    "next-auth": "^4.24.11",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.9.16",
    "openai": "^4.104.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "pino": "^10.0.0",
    "pino-pretty": "^13.1.1",
    "prismjs": "^1.30.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "reflect-metadata": "^0.2.2",
    "rehype-highlight": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "sharp": "^0.34.4",
    "strip-comments": "^2.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.29.3",
    "tailwind-merge": "^2.5.4",
