{
  "name": "ai-saas-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean-comments": "node scripts/removeComments.cjs",
    "clean-comments:inplace": "node scripts/removeComments.cjs --in-place",
    "dev": "next dev",
    "prisma:generate": "prisma generate",
    "build": "prisma generate && next build",
    "start": "next start -p $PORT",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "// Database Commands": "",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "tsx scripts/setup-database.ts",
    "db:reset": "tsx scripts/reset-database.ts",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:setup": "npm run db:generate && npm run db:migrate && npm run db:seed",
    "// Fix & Maintenance Commands": "",
    "fix:imports": "node scripts/fix-imports.js",
    "fix:all": "npm run fix:imports && npm run type-check",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "npm run clean && rm -rf node_modules && npm install",
    "// Testing Commands": "",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:integration": "jest --testPathPattern=integration",
    "test:auth": "tsx scripts/test-auth.ts",
    "test:db": "tsx scripts/test-database.ts",
    "test:email": "node src/app/api/auth/scripts/test-mail.js",
    "// Development Helpers": "",
    "mailhog": "docker run -d -p 1025:1025 -p 8025:8025 mailhog/mailhog",
    "ngrok": "ngrok http 3000",
    "// Environment & Deployment": "",
    "env:verify": "tsx scripts/verify-env.ts",
    "env:verify:strict": "tsx scripts/verify-env.ts --strict",
    "// Production Commands": "",
    "build:prod": "npm run env:verify && npm run db:generate && npm run db:migrate:prod && npm run build",
    "start:prod": "npm run build:prod && npm run start",
    "deploy:verify": "npm run env:verify:strict && npm run type-check && npm run build",
    "verify:production": "tsx scripts/post-deploy-verifier.ts",
    "verify:production:verbose": "tsx scripts/post-deploy-verifier.ts --verbose",
    "conn:check": "tsx scripts/conn-check.ts",
    "conn:check:verbose": "tsx scripts/conn-check.ts --verbose"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@google/generative-ai": "^0.21.0",
    "@payos/node": "^2.0.3",
