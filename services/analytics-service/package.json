{
  "name": "analytics-service",
  "version": "1.0.0",
  "description": "Analytics Service for tracking user activity, chat usage, and revenue metrics",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "node src/database/migrate.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": ["analytics", "microservices", "clickhouse", "rabbitmq"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@clickhouse/client": "^1.7.0",
    "amqplib": "^0.10.4",
    "express": "^4.21.1",
    "dotenv": "^16.4.5",
    "cors": "^2.8.5",
    "helmet": "^8.0.0",
    "prom-client": "^15.1.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "@types/cors": "^2.8.17",
    "@types/amqplib": "^0.10.5",
    "typescript": "^5.7.2",
    "ts-node-dev": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.14",
    "ts-jest": "^29.2.5"
  }
}
